@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{
   
}

<section class="hero @Model.Rendering.Parameters["BackgroundColor"] left @Model.Rendering.Parameters["CircleColor"]" id="hero-services">
    <div class="hero-background-container">
        @Html.Sitecore().Field("Image")
    </div>
    <div class="color-container left">
        <h2 class="content-circle">@Html.Sitecore().Field("Content Intro")</h2>
        <i class="icon icon-blockangle"></i>
        <div class="hero-content">
            <h1>@Html.Sitecore().Field("Content Heading")</h1>
        </div>
    </div>
</section>