@{
    Sitecore.Data.Fields.MultilistField otherlinksfield = Model.Item.Fields["items"];
    var otheritems = otherlinksfield.GetItems();
}
<section class="cta-boxes" id="partners-boxes">
    <div class="container">
        <div class="row">
            <div class="col-xs-12 section-header box-section-header">
                <h2><strong>@Html.Sitecore().Field("titleBold")</strong> @Html.Sitecore().Field("titleNormal")</h2>
                <aside>
                    @Html.Sitecore().Field("Description")
                </aside>
            </div>
        </div>
        <div class="row box-wrapper">

            @foreach (Sitecore.Data.Items.Item item in otheritems)
            {
                Sitecore.Data.Fields.ImageField imageRef = item.Fields["boxLogoImage"];
                string imgSrc = Sitecore.Resources.Media.MediaManager.GetMediaUrl(imageRef.MediaItem);
                <div class="box-container col-lg-3 col-md-6 col-sm-6 col-xs-12">
                    <div class="box-partner box-@Html.Sitecore().Field("buttonText", item)">
                        <a href="/partners/ibm">
                            @Html.Sitecore().Field("boxBackgroundImage", item, new { @class = "box-background img-responsive" })
                            <div class="box-overlay-container box-overlay-standard">
                                <div class="box-logo-container">
                                    <img src="@imgSrc" class="box-logo img-responsive" alt="">
                                </div>
                            </div>
                            <div class="box-overlay-container box-overlay-hover">
                                @Html.Sitecore().Field("Image", item, new { @class = "box-prft-logo img-responsive" })
                                <h5>
                                    <b>
                                        @Html.Sitecore().Field("titleBold", item)
                                    </b>
                                    @Html.Sitecore().Field("Content Heading", item)
                                </h5>
                                <p></p><p>
                                    @Html.Sitecore().Field("Content Intro", item)
                                </p><p></p>
                                <div class="button-wrapper">
                                    <div class="btn-box">
                                        @Html.Sitecore().Field("buttonText", item)
                                    </div>
                                </div>
                            </div>
                        </a>
                    </div>
                </div>
            }
        </div>
    </div>
</section>